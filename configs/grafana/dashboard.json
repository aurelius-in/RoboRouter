{
  "title": "RoboRouter API Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP Request Rate",
      "targets": [
        { "expr": "sum(rate(roborouter_requests_total[5m]))" }
      ]
    },
    {
      "type": "graph",
      "title": "Latency (p95)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(roborouter_request_latency_seconds_bucket[5m])) by (le))" }
      ]
    },
    {
      "type": "graph",
      "title": "Exports per minute",
      "targets": [
        { "expr": "sum(rate(roborouter_exports_total[5m])) by (type)" }
      ]
    }
  ],
  "schemaVersion": 27,
  "version": 1
}


