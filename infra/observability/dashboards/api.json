{
  "title": "RoboRouter API",
  "schemaVersion": 38,
  "version": 1,
  "timezone": "browser",
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests by endpoint",
      "gridPos": { "w": 24, "h": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(roborouter_requests_total[1m]))",
          "legendFormat": "{{endpoint}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "gridPos": { "w": 24, "h": 8, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(roborouter_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p95"
        }
      ]
    }
  ]
}

